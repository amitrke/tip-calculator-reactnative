{
  "name": "tip-calculator-reactnative",
  "version": "3.4.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "release-it": "release-it",
    "test": "echo \"No tests specified\" && exit 0",
    "maestro:test": "./maestro/run_maestro_tests.sh",
    "maestro:test:android": "./maestro/run_maestro_tests.sh android",
    "maestro:test:ios": "./maestro/run_maestro_tests.sh ios",
    "maestro:screenshots": "maestro test maestro/ --test-output-dir maestro/screenshots",
    "screenshots": "maestro test maestro/ --test-output-dir maestro/screenshots",
    "organize:screenshots": "./organize_screenshots.sh",
    "build:android:local": "eas build -p android --profile preview --local",
    "build:android:cloud": "eas build -p android --profile production",
    "build:ios:local": "eas build -p ios --profile preview --local",
    "build:ios:cloud": "eas build -p ios --profile production",
    "build:all:local": "npm run build:android:local && npm run build:ios:local",
    "build:all:cloud": "npm run build:android:cloud && npm run build:ios:cloud"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.0",
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.18",
    "@gluestack-ui/themed": "^1.1.22",
    "@react-native-community/cli": "latest",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~53.0.22",
    "expo-font": "~13.3.2",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.542.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "styled-components": "^6.1.11",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
